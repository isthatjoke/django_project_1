f %}\n                    {% else %}\n                    {% if field.value %}\n                    <span class=\"orderitems-{{ forloop.parentloop.counter0 }}-price\">\n                        {{ field.value }}\n                    </span> $\n                    {% endif %}\n                    {% endif %}\n                    </td>\n                {% endfor %}\n                </tr>\n                {% endfor %}\n                </table>\n            <input class=\"form-control btn btn-outline-success\" type=\"submit\" value=\"save\">\n\n           {% include 'ordersapp/includes/inc_order_actions.html' %}\n            </form>\n        </div>\n    </div>\n    </div>\n\n{% endblock %}
